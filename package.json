{
  "name": "heartbeats",
  "version": "0.0.0",
  "description": "Heartbeats visualization for some vertebrate",
  "main": "./dist/index.html",
  "scripts": {
    "start": "./bin/start.sh",
    "build": "./bin/build.sh",
    "test": "cross-env NODE_ENV=test NODE_PATH=src:test mocha",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "lint": "npm run lint:style && npm run lint:es",
    "lint:style": "stylelint  './src/**/*.js' './src/**/*.jsx'",
    "lint:es": "eslint --cache --ext=jsx --ext=js .",
    "lint:prettier":
      "prettier-eslint \"./src/**/*.js\" \"./src/**/*.jsx\" \"./test/**/*.js\" \"./test/**/*.jsx\" --write",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "history": "^4.7.2",
    "immutability-helper": "^2.4.0",
    "prop-types": "^15.6.0",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "styled-components": "^2.2.0",
    "switz": "^0.0.20"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.11.6",
    "chai": "^4.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "enzyme": "^2.8.2",
    "eslint": "^4.10.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-eslint-comments": "^2.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^0.11.2",
    "git-revision-webpack-plugin": "^2.5.1",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "jsdom": "9.9.1",
    "json-loader": "^0.5.4",
    "lcov-parse": "^1.0.0",
    "mocha": "^3.5.0",
    "mocha-jsdom": "^1.1.0",
    "nyc": "^11.1.0",
    "prettier": "^1.7.4",
    "prettier-eslint": "^8.2.1",
    "prettier-eslint-cli": "^4.4.0",
    "react-addons-perf": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "react-transform-hmr": "^1.0.4",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "lint-staged": {
    "*.{js,jsx,json}": ["prettier --write", "git add"]
  },
  "nyc": {
    "include": ["src/*.js", "src/*.jsx", "src/**/*.js", "src/**/*.jsx"],
    "require": ["babel-register"],
    "sourceMap": false,
    "instrument": false
  }
}
